{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["plugin:vue/vue3-recommended", "prettier"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2015,
    "parser": "@typescript-eslint/parser",
    "sourceType": "module"
  },
  "plugins": ["vue", "@typescript-eslint", "prettier"],
  "rules": {
    "prettier/prettier": "error",
    "vue/multi-word-component-names": 0
  },
  "settings": {}
}
